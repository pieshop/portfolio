{
  "name": "portfolio-api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development nodemon ./src/bin/www",
    "deploy_dev": "babel-node tools/run deploy --env.target=local",
    "deploy_dist": "babel-node tools/run deploy --env.target=live"
  },
  "dependencies": {
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.6.1",
    "eslint-config-google": "^0.10.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "nodemon": "^1.18.5",
    "prettier": "^1.14.3",
    "rsyncwrapper": "^3.0.1"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-runtime",
      "transform-object-rest-spread"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "iOS > 7",
    "not ie <= 8"
  ],
  "engines": {
    "node": ">8.1.x",
    "npm": ">5.0.x"
  }
}
